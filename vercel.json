{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/main.jsx",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/sendMsg",
      "dest": "/api/sendMsg"
    },
    {
      "src": "/admin",
      "dest": "/api/admin"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}