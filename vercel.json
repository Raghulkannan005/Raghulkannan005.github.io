{
  "version": 2,
  "builds": [
    {
      "src": "api/sendMsg.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/admin.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/sendMsg",
      "destination": "/api/sendMsg"
    },
    {
      "source": "/api/admin",
      "destination": "/api/admin"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.js)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    }
  ]
}
